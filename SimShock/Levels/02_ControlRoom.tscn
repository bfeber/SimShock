[sub_resource type="GDScript" id="GDScript_8bk86"]
script/source = "extends Node

signal power_module_restored(power_module : Node3D)

@export var lamps : Array[CogitoSwitch]
@export var buttons : Array[CollisionObject3D]
@export var audio_stream_player : AudioStreamPlayer3D

func check_power_module_restored() -> void:
	for lamp in lamps:
		if not lamp.is_on:
			return

	for button in buttons:
		button.collision_layer = 1

	power_module_restored.emit(self)
	audio_stream_player.play()
"

