[sub_resource type="GDScript" id="GDScript_1n10i"]
script/source = "extends Node

@export var required_power_modules : Array[Node3D]
@export var buttons : Array[CogitoSwitch]
@export var power_label : Label3D
@export var power_monitor : CSGBox3D
@export var power_monitor_restored_material : Material


func _on_power_module_restored(power_module : Node3D) -> void:
	required_power_modules.erase(power_module)
	if required_power_modules.is_empty():
		for button in buttons:
			button.needs_item_to_operate = false
		power_label.text = \"Power Online\"
		power_monitor.material = power_monitor_restored_material


func _on_offices_access_button_switched():
	FacilityState.offices_access = true


func _on_storage_access_button_switched():
	FacilityState.storage_access = true
"

